{
  "name": "rivian-cli",
  "version": "0.0.0",
  "description": "Rivian CLI",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/tmack8001/rivian-cli",
  "bugs": "https://github.com/tmack8001/rivian-cli/issues",
  "repository": "tmack8001/rivian-cli",
  "license": "MIT",
  "author": "Trevor Mack @tmack8001",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "rivian": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "rivian",
    "commands": "./dist/commands",
    "dirname": "rivian",
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Authenticate to the Rivian Cloud API"
      },
      "charging": {
        "description": "fetch user or specific vehicle charging session information"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "node-persist": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/json2csv": "^5.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/node-fetch": "^2.6.11",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-persist": "^3.1.8",
    "@types/uuid": "^9.0.8",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "mocha": "^10",
    "node-localstorage": "^3.0.5",
    "oclif": "^4.12.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
